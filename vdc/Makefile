	-$(DEL_FILE) Makefile
install_subtargets: sub-cpu-install_subtargets sub-vdc-install_subtargets sub-vce-install_subtargets sub-psg-install_subtargets FORCE
uninstall_subtargets: sub-cpu-uninstall_subtargets sub-vdc-uninstall_subtargets sub-vce-uninstall_subtargets sub-psg-uninstall_subtargets FORCE

sub-cpu-check:
	@test -d cpu/ || mkdir -p cpu/
	cd cpu/ && ( test -e Makefile || $(QMAKE) /home/gravis/projects/epceemu/cpu/cpu.pro -spec epiphany-g++ -o Makefile ) && $(MAKE) -f Makefile check
