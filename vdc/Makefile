sub-psg-check:
	@test -d psg/ || mkdir -p psg/
	cd psg/ && ( test -e Makefile || $(QMAKE) /home/gravis/projects/epceemu/psg/psg.pro -spec epiphany-g++ -o Makefile ) && $(MAKE) -f Makefile check
check: sub-cpu-check sub-vdc-check sub-vce-check sub-psg-check
install: install_subtargets  FORCE

uninstall:  uninstall_subtargets FORCE

FORCE:

